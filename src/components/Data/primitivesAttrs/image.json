{
  "name": "feImage",
  "inputsData": {
    "xlinkHref": {
      "type": "text",
      "name": "xlink:href"
    },
    "preserveAspectRatio": {
      "type": "select",
      "double": true,
      "valuesKeys": [
        "align",
        "meetOrSlice"
      ]
    },
    "crossOrigin": {
      "type": "select"
    },
    "x":{
      "type": "number"
    },
    "y":{
      "type": "number"
    },
    "width":{
      "type": "number"
    },
    "height":{
      "type": "number"
    }
  },
  "align": [
    "xMinYMin",
    "xMinYMid",
    "xMinYMax",

    "xMidYMin",
    "xMidYMid",
    "xMidYMax",

    "xMaxYMin",
    "xMaxYMid",
    "xMaxYMax"
  ],
  "crossOrigin": [
    "anonymous",
    "use-credentials"
  ],
  "meetOrSlice": [
    "meet",
    "slice"
  ]
}
